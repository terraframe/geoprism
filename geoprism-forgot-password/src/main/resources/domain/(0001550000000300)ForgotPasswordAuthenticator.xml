<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdUtil
         name="net.geoprism.ForgotPasswordAuthenticator"
         label="Forgot Password Authenticator">
        <mdMethod
           name="initiate"
           label="initate"
           static="false"
           returnType="void">
          <mdParameter name="username" label="username" order="0" type="java.lang.String" />
        </mdMethod>
        <mdMethod
           name="complete"
           label="complete"
           static="false"
           returnType="void">
          <mdParameter name="token" label="token" order="0" type="java.lang.String" />
          <mdParameter name="newPassword" label="newPassword" order="1" type="java.lang.String" />
        </mdMethod>
      </mdUtil>
    </create>
    <update></update>
    <permissions>
      <role
         roleName="PUBLIC">
        <grant>
          <mdBusinessPermission
             type="net.geoprism.ForgotPasswordAuthenticator">
            <mdMethodPermission
               methodName="initiate">
              <operation name="EXECUTE" />
            </mdMethodPermission>
            <mdMethodPermission
               methodName="complete">
              <operation name="EXECUTE" />
            </mdMethodPermission>
          </mdBusinessPermission>
        </grant>
      </role>
      <method
         methodName="initiate"
         type="net.geoprism.ForgotPasswordAuthenticator">
        <grant>
          <mdBusinessPermission
             type="net.geoprism.ForgotPasswordRequest">
            <operation name="ALL" />
          </mdBusinessPermission>
          <mdBusinessPermission
             type="net.geoprism.EmailSetting">
            <operation name="ALL" />
          </mdBusinessPermission>
          <mdBusinessPermission
             type="net.geoprism.GeoprismUser">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
        </grant>
      </method>
      <method
         methodName="complete"
         type="net.geoprism.ForgotPasswordAuthenticator">
        <grant>
          <mdBusinessPermission
             type="net.geoprism.ForgotPasswordRequest">
            <operation name="ALL" />
          </mdBusinessPermission>
          <mdBusinessPermission
             type="net.geoprism.GeoprismUser">
            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
            <operation name="WRITE" />
            <operation name="WRITE_ALL_ATTRIBUTES" />
          </mdBusinessPermission>
        </grant>
      </method>
    </permissions>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>