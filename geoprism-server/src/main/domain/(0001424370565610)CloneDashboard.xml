          <mdMethod name="clone" returnType="net.geoprism.Dashboard" label="Clone" static="false" >
            <mdParameter name="name" label="name" order="1" type="java.lang.String" />
          </mdMethod>
        </create>
      </mdBusiness>
    </update>

  </doIt>
  <undoIt>
    <delete></delete>
