<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <delete>
      <object type="com.runwaysdk.system.metadata.MdEdge" key="net.geoprism.registry.graph.GeoVertexHasSynonym" />
      <object type="com.runwaysdk.system.metadata.MdEdge" key="net.geoprism.registry.graph.ExternalID" />
      <object type="com.runwaysdk.system.metadata.MdGeoVertex" key="net.geoprism.registry.graph.GeoVertex" />
    </delete>
    <create>
      <mdVertex name="net.geoprism.registry.graph.GeoVertex" label="Geo Vertex" isAbstract="true" enableChangeOverTime="false">
          <attributes>
            <dateTime name="lastUpdateDate" label="Last Update Date" />
            <dateTime name="createDate" label="Create Date" />
          </attributes>
      </mdVertex>

      <mdEdge name="net.geoprism.registry.graph.ExternalID" label="External ID" parent="net.geoprism.registry.graph.ExternalSystem" child="net.geoprism.registry.graph.GeoVertex" generateSource="false">
        <attributes>
          <text name="id" label="ID" required="true" />
          <text name="key" label="Key" required="false" />
        </attributes>
      </mdEdge>

      <mdEdge name="net.geoprism.registry.graph.GeoVertexHasSynonym" label="GeoVertex Has Synonym" parent="net.geoprism.registry.graph.GeoVertex" child="net.geoprism.registry.graph.GeoVertexSynonym" generateSource="true">
      </mdEdge>

    </create>
    
    <permissions>
      <role roleName="geoprism.admin.Administrator">
        <grant>
          <mdBusinessPermission type="net.geoprism.registry.graph.GeoVertex">
            <operation name="ALL" />
          </mdBusinessPermission>
          <mdRelationshipPermission type="net.geoprism.registry.graph.ExternalID">
            <operation name="ALL" />
          </mdRelationshipPermission>
          <mdRelationshipPermission type="net.geoprism.registry.graph.GeoVertexHasSynonym">
            <operation name="ALL" />
          </mdRelationshipPermission>
        </grant>
      </role>
    </permissions>
    
  </doIt>
  <undoIt>
    <delete>

    </delete>
  </undoIt>
</version>